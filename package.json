{
  "name": "luapeck",
  "version": "4.1.0",
  "description": "Packaging multiple lua files into one file",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test:build": "node bin/luapeck -o test/packed.lua build test/test.lua",
    "test:start": "lua test/packed.lua",
    "test": "npm run test:build && npm run test:start",
    "prep": "node bin/luapeck -p ./test"
  },
  "bin": {
    "luapeck": "bin/luapeck"
  },
  "author": "Shichiha",
  "dependencies": {
    "chalk": "^5.0.1",
    "inquirer": "^9.1.0",
    "md5-hex": "^4.0.0"
  }
}
